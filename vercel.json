{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },

    { "src": "/(favicon\\.ico|robots\\.txt|manifest\\.json)", "dest": "/$1" },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
